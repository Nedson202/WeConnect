    "migration:dev": "sequelize db:migrate",
    "migrate:test": "NODE_ENV=test npm run migration:undo:test && npm run migration:test && npm run seed:undo && npm run migration:test && npm run seed && npm run test:nyc",
    "migration:undo:test": "NODE_ENV=test ./node_modules/.bin/sequelize db:migrate:undo:all",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "build": "webpack",
    "seed:undo": "NODE_ENV=test sequelize db:seed:undo:all",
    "seed": "NODE_ENV=test sequelize db:seed:all",
    "seeders": "./node_modules/.bin/sequelize db:seed:undo:all && sequelize db:seed:all",
    "postinstall": "npm run seeders"
